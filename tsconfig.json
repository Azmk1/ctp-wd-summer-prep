{
  "compilerOptions": {
    "target": "es2018",
    "lib": [
      "DOM",
      "ES2019"
    ],
    "jsx": "react-jsx",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowJs": true,
    "allowUmdGlobalAccess": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
